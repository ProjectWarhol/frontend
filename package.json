{
  "name": "warhol-frontend",
  "version": "0.0.1",
  "description": "This is the Project Warhol mobile application",
  "keywords": [
    "react",
    "react-native",
    "nft",
    "blockchain",
    "web3",
    "solidity",
    "social-media"
  ],
  "main": "node_modules/expo/AppEntry.js",
  "author": "Project Warhol",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ProjectWarhol/frontend"
  },
  "scripts": {
    "lint": "eslint",
    "format": "npx prettier --config .prettierrc -w .",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "solHint": "solhint 'contracts/**/*.sol'",
    "compile": "yarn solHint && truffle compile",
    "test": "yarn solHint && truffle test",
    "mocha": "mocha 'test/**/*.js'|| true",
    "mDev": "yarn solHint && truffle migrate --network development",
    "mOnDev": "yarn solHint && truffle migrate --network onlineDevelopment",
    "mStaging": "yarn solHint && truffle migrate --network staging"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.0.8",
    "bip39": "^3.0.4",
    "expo": "^44.0.0",
    "expo-status-bar": "~1.2.0",
    "prettier": "^2.5.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "^17.0.2",
    "react-native": "^0.64.3",
    "react-native-crypto": "^2.2.0",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-randombytes": "^3.6.1",
    "react-native-reanimated": "^2.4.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-web": "0.17.1",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4",
    "react-navigation-tabs": "^2.11.2",
    "rn-nodeify": "^10.3.0",
    "web3": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@openzeppelin/contracts": "^4.5.0",
    "@truffle/hdwallet-provider": "^2.0.3",
    "chai": "^4.3.6",
    "dotenv": "^16.0.0",
    "eslint": "^7.12.1",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.2",
    "mocha": "^9.2.1",
    "solhint": "^3.3.7"
  }
}
